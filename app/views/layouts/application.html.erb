<!DOCTYPE html>
<html lang="en">

  <head>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="Bootstrap Admin App + Ruby on Rails">
    <meta name="keywords" content="app, responsive, jquery, bootstrap, dashboard, admin">
    <title>RajinBelajar</title>
    <!-- =============== VENDOR STYLES ===============-->
    <%= stylesheet_link_tag 'base' %>
    <!-- =============== VIEW VENDOR STYLES ===============-->
    <%= stylesheet_link_tag 'dashboard' %>
    <%= yield :vendor_css_area %>
    <!-- =============== APP STYLES ===============-->
    <%= stylesheet_link_tag 'application', media: 'all', id: "maincss", 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'angle/themes/theme-h' %>
    <!-- =============== Datatables ===============-->
	 	<%= stylesheet_link_tag 'datatables/dataTables.bootstrap4.css' %>
	 	<%= stylesheet_link_tag 'datatables/keyTable.bootstrap.css' %>
	 	<%= stylesheet_link_tag 'datatables/responsive.bootstrap.css' %>
    <%= javascript_include_tag "application", 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag "plugins/chart.js/dist/Chart", 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/gmaps.js/0.4.25/gmaps.js", 'data-turbolinks-track': 'reload' %>
	 	<%= javascript_include_tag "custom", 'data-turbolinks-track': 'reload' %>
    <script src="https://www.gstatic.com/firebasejs/5.9.1/firebase.js"></script>
    <!-- GOOGLE MAPS-->
    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyC5q3SCYpG9uLQxyka0hHv5e4r9lPAseYg&libraries=places" %>
  </head>

  <body>
    <div class="wrapper">
      <!-- top navbar-->
      <header class="topnavbar-wrapper">
        <%= render partial: "layouts/partials/topnavbar" %>
      </header>
      <!-- sidebar-->
      <aside class="aside-container">
        <%= render partial: "layouts/partials/sidebar" %>
      </aside>
      <!-- offsidebar-->
      <aside class="offsidebar d-none">
        <%= render partial: "layouts/partials/offsidebar" %>
      </aside>
      <!-- Main section-->
      <section class="section-container">
        <!-- Page content-->
        <div class="content-wrapper">
          <%#= render "layouts/partials/flash_message" %>
          <%= yield %>
        </div>
      </section>
      <!-- Page footer-->
      <footer class="footer-container">
        <%= render partial: "layouts/partials/footer" %>
      </footer>
    </div>
    <!-- =============== VENDOR SCRIPTS ===============-->
    <%= javascript_include_tag 'base' %>
    <!-- =============== VIEW VENDOR SCRIPTS ===============-->
    <%= javascript_include_tag 'dashboard' %>
    <%= yield :vendor_js_area %>
    <!-- =============== APP SCRIPTS ===============-->

    <%= yield :javascripts %>
  </body>

</html>
